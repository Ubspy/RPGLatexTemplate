% Package for setting background image
\RequirePackage{background}

% Required for some xcolor functionality
\RequirePackage{colortbl}

% Make it so we can have stylalized lists
\RequirePackage{enumitem}

% Allows for left right and center justification
\RequirePackage{ragged2e}

% Helps to set page geometry like margins and such
\RequirePackage{geometry}

% Make it so we can have more variation on headers and footers
\RequirePackage {fancyhdr}

% Load the font encoder package, required to render custom fonts
\RequirePackage[T1]{fontenc}

% For drawing lines and other special shapes
\RequirePackage{tikz}
\usetikzlibrary{positioning}
\usetikzlibrary{calc}

% Create styled textboxes
\RequirePackage [breakable,skins,xparse]{tcolorbox}

% For making tables with specific spacing
\RequirePackage{tabularx}

% To get section and chapter names
\RequirePackage{titlesec}

% Package for defining all the colors
\RequirePackage{xcolor}

\RequirePackage{lib/colors}
\RequirePackage{lib/constants}
\RequirePackage{lib/fonts}

\RequirePackage{lib/boxes}
\RequirePackage{lib/footers}
