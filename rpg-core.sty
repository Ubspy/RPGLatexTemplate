% To get section and chapter names
\RequirePackage{titlesec}

% Package for defining all the colors
\RequirePackage{xcolor}

\RequirePackage{lib/colors}
\RequirePackage{lib/constants}
\RequirePackage{lib/fonts}

\RequirePackage{lib/footers}
\RequirePackage{lib/page-style}
\RequirePackage{lib/sections}